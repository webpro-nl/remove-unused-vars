{
  "name": "remove-unused-vars",
  "version": "0.0.9",
  "description": "Remove more unused variables",
  "type": "module",
  "main": "./index.js",
  "bin": "./index.js",
  "license": "ISC",
  "author": {
    "email": "lars@webpro.nl",
    "name": "Lars Kappert",
    "url": "https://webpro.nl"
  },
  "files": [
    "index.js"
  ],
  "repository": "github:webpro-nl/remove-unused-vars",
  "dependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "release": "release-it",
    "test": "node --test ./test/*.test.js",
    "test:eslint": "eslint -c fixtures/eslint.config.js --rule 'no-unused-vars: error' --quiet -f json fixtures | ./index.js",
    "test:ts-eslint": "eslint -c fixtures/eslint.config.js --rule 'no-unused-vars: off' --rule '@typescript-eslint/no-unused-vars: error' --quiet -f json fixtures | ./index.js",
    "test:biome": "biome lint --only correctness/noUnusedVariables --only correctness/noUnusedImports --only correctness/noUnusedFunctionParameters --reporter json fixtures",
    "test:oxlint": "oxlint -A all -D 'no-unused-vars' -f json fixtures | ./index.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "eslint": "^9.39.1",
    "oxlint": "^1.29.0",
    "release-it": "^19.0.6",
    "typescript-eslint": "^8.47.0"
  }
}
